/* 收藏选择标签  */

.show-tips {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  height: 100vh;
  overflow: hidden;
  /* z-index: 99; */
  z-index: -1;
  opacity: 0;
}

.show-tips.show {
  z-index: 99;
  opacity: 1;
}

.show-tips.show .wrap {
  transform: translate(-50%, -50%) scale(1);
}

.show-tips .wrap {
  width: 450rpx;
  /* height: 450rpx; */
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%) scale(0.3);
  transform-origin: center;
  background-color: #fff;
  padding: 30rpx;
  border-radius: 15rpx;
  z-index: 99;
  transition: transform 0.3s;
}

.show-tips .list {
  max-height: 400rpx;
  overflow-y: auto;
  margin-top: 20rpx;
  margin-bottom: 10rpx;
  z-index: 99;
}

.show-tips .button {
  border-top: 1px solid #d7dced;
  /* border-bottom:1px solid #d7dced;  */
  padding-top: 15rpx;
  box-sizing: content-box;
}

.show-tips .button view {
  display: inline-block;
  width: 50%;
  text-align: center;
  box-sizing: border-box;
}

.show-tips .title {
  text-align: center;
  font-size: 34rpx;
  color: #333;
}

.show-tips .wrap {
  position: relative;
}

.show-tips .wrap .row {
  /* text-align: center; */
  letter-spacing: 3rpx;
  font-style: normal;
  padding: 15rpx 0;
  border-bottom: 1px solid #d7dced;
}

.show-tips .wrap .row .select {
  width: 40rpx;
  height: 40rpx;
  vertical-align: middle;
  margin-left: 30rpx;
  float: right;
  margin-right: 30rpx;
}

.show-tips .wrap .row .select.not {
  border-radius: 50%;
  border: 1px solid #8ac5e6;
}

.show-tips .wrap .row text {
  padding-left: 20rpx;
}

.show-tips .wrap .row.current {
}

.show-tips .wrap .row:last-child {
  border: none;
}

.show-tips .button view:first-child {
  border-right: 1px solid #d7dced;
}

.show-tips .wrap .close {
  position: absolute;
  width: 60rpx;
  height: 60rpx;
  right: -20rpx;
  top: -20rpx;
  background-color: #fff;
  border-radius: 50%;
}

.mask {
  background-color: #3a4053;
  opacity: 0.8;
  position: fixed;
  z-index: 99;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}

