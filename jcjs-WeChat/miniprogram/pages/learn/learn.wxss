}

.keyboard .btn {
  width: 33.33%;
  height: 120rpx;
  line-height: 120rpx;
  text-align: center;
  background-color: #fff;
  border-radius: 4rpx;
  padding: 5rpx;
  box-sizing: border-box;
}

.keyboard .btn .n {
  width: 100%;
  height: 100%;
  background-color: #eee;
  transition: all 0.3s;
}

.keyboard .btn .n:active {
  background: #e0e0e0;
  transform: scale(0.97);
}

.l-bg, .l-bg image {
  width: 100%;
  vertical-align: top;
}

/* section  */

.section {
  width: 100%;
  text-align: center;
  padding: 20rpx;
  box-sizing: border-box;
}

.section .picker image {
  width: 45rpx;
  height: 25rpx;
  margin-right: 15rpx;
  /* vertical-align: middle; */
}

.section .bubble {
  width: 100%;
  overflow: hidden;
  word-break: normal;
  white-space: nowrap;
  word-wrap: normal;
  margin: 0 auto;
}

.section .wrap .bubble-num image {
  vertical-align: middle;
  display: inline-block;
  margin: 0;
}
.section .wrap .bubble-num .one_bullel{
  width: 50rpx;
  height: 64rpx;
}
.section .wrap .bubble-num .ico-help{
  width: 40rpx;
  height: 48rpx;
  margin-top:-5px;
  padding-left:5rpx;  
}
.section .bubble image {
  width: 100%;
  height: 100%;
  vertical-align: top;
}

.section .bubble .box {
  display: inline-block;
  width: 50rpx;
  height: 64rpx;
  margin: 0;
  text-align: center;
}

.section .bubble .box.opacity {
  opacity: 0.18;
}

.section .radio-group {
  align-items: center;
  justify-content: space-between;
}

.section .radio-group .radio {
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  border: 4rpx solid #bbb;
  border-radius: 50%;
  position: relative;
  margin-right: 10rpx;
  vertical-align: middle;
  box-sizing: border-box;
  transition: all 0.3s;
}

.section .radio-group .txt {
  margin-left: 30rpx;
  font-size: 32rpx;
}

.section .wrap .bubble-num {
  display: inline-block;
  /* display: flex; */
  align-items:center;
  justify-content:center;
}

.section .radio-group .radio.current {
  border-color: #259b24;
}

.section .radio-group .radio.current .dot {
  background-color: #259b24;
}

.section .radio-group .radio .dot {
  position: absolute;
  width: 100%;
  height: 100%;
  border-radius: 50%;
  background-color: #fff;
  transform: scale(0.65);
  transition: inherit;
}

.section .item {
  justify-content: space-between;
  align-items: center;
  color: #fff;
  text-align: center;
  margin-top: 34rpx;
}

.section .item .left {
  width: 100%;
  height: 80rpx;
  margin-right: 30rpx;
  line-height: 80rpx;
  border-radius: 4rpx;
  box-shadow: 0 15rpx 15px 0 rgba(0, 0, 0, 0.24);
}

.section .item .right {
  width: 550rpx;
  height: 45rpx;
  line-height: 45rpx;
  border: 1px solid #bbb;
  color: black;
  position: relative;
  z-index: 9;
}

.section .item .progress {
  position: absolute;
  /* width: 100%; */
  height: 100%;
  left: 0;
  width: 50%;
  top: 0;
  z-index: -1;
  transition: width 0.35s;
}


